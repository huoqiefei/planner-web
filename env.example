# API Configuration
VITE_API_BASE_URL=https://board.centrekit.com/index.php/planner/api

# Documentation Sources
VITE_DOC_MANUAL_URL=manual.md
VITE_DOC_ABOUT_URL=about.md

# Activity Limits (number of activities allows)
VITE_LIMIT_TRIAL=20
VITE_LIMIT_LICENSED=100
VITE_LIMIT_PREMIUM=500
VITE_LIMIT_ADMIN=9999

# Resource Limits
VITE_LIMIT_RES_TRIAL=10
VITE_LIMIT_RES_LICENSED=50
VITE_LIMIT_RES_PREMIUM=200
VITE_LIMIT_RES_ADMIN=9999

# Cloud Project Limits
VITE_LIMIT_CLOUD_TRIAL=1
VITE_LIMIT_CLOUD_LICENSED=10
VITE_LIMIT_CLOUD_PREMIUM=100
VITE_LIMIT_CLOUD_ADMIN=9999

# Custom Field Limits
VITE_LIMIT_CF_TRIAL=0
VITE_LIMIT_CF_LICENSED=5
VITE_LIMIT_CF_PREMIUM=20
VITE_LIMIT_CF_ADMIN=9999

# Cloud Features Toggle (set to false to disable cloud save/load)
VITE_ENABLE_CLOUD_FEATURES=false



# Feature Permissions (comma-separated roles: trial, licensed, premium, admin)
VITE_FEATURE_ROLES_EXPORT=licensed,premium,admin
VITE_FEATURE_ROLES_PRINT=trial,licensed,premium,admin
VITE_FEATURE_ROLES_CLOUD_SAVE=trial,licensed,premium,admin
VITE_FEATURE_ROLES_CLOUD_LOAD=trial,licensed,premium,admin
